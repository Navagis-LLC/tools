<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] -Register Maps Project - Navagis Maps</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
          rel="stylesheet"
          type="text/css" />
</head>
<body>
    @Html.AntiForgeryToken()
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="@Url.Action("Index", "Home")" id="loginLink" style="color:lightgreen">
                    <img src="~/images/navagislogo.png" width="200" />
                </a>
            </div>
            <div class="navbar-collapse collapse">

            </div>
        </div>
    </div>

    <partial name="_CookieConsentPartial" />
    <br />
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>
                &copy;
                <script>document.write(new Date().getFullYear())</script> - Navagis Inc
            </p>
        </footer>
    </div>

    @*</div>*@

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
